#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/detect-port-alt@1.1.6/node_modules/detect-port-alt/bin/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/detect-port-alt@1.1.6/node_modules/detect-port-alt/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/detect-port-alt@1.1.6/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules:/home/workspace/react-hooks/sessions/node_modules:/home/workspace/react-hooks/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/detect-port-alt/bin/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/detect-port-alt/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/detect-port-alt@1.1.6/node_modules/detect-port-alt/bin/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/detect-port-alt@1.1.6/node_modules/detect-port-alt/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/detect-port-alt@1.1.6/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules:/home/workspace/react-hooks/sessions/node_modules:/home/workspace/react-hooks/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/detect-port-alt/bin/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/detect-port-alt/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../detect-port-alt/bin/detect-port" "$@"
else
  exec node  "$basedir/../detect-port-alt/bin/detect-port" "$@"
fi
