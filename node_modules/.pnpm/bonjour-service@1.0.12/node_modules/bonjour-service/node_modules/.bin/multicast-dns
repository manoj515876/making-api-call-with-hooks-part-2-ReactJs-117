#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/multicast-dns@7.2.4/node_modules/multicast-dns/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/multicast-dns@7.2.4/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules:/home/workspace/react-hooks/sessions/node_modules:/home/workspace/react-hooks/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/bonjour-service@1.0.12/node_modules/bonjour-service/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/bonjour-service@1.0.12/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/multicast-dns@7.2.4/node_modules/multicast-dns/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/multicast-dns@7.2.4/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules:/home/workspace/react-hooks/sessions/node_modules:/home/workspace/react-hooks/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/bonjour-service@1.0.12/node_modules/bonjour-service/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/bonjour-service@1.0.12/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../multicast-dns@7.2.4/node_modules/multicast-dns/cli.js" "$@"
else
  exec node  "$basedir/../../../../../multicast-dns@7.2.4/node_modules/multicast-dns/cli.js" "$@"
fi
