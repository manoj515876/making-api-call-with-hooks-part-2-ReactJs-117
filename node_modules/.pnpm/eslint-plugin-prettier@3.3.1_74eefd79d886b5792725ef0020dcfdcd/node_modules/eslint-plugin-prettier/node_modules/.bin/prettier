#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/prettier@2.5.1/node_modules/prettier/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/prettier@2.5.1/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules:/home/workspace/react-hooks/sessions/node_modules:/home/workspace/react-hooks/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/eslint-plugin-prettier@3.3.1_74eefd79d886b5792725ef0020dcfdcd/node_modules/eslint-plugin-prettier/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/eslint-plugin-prettier@3.3.1_74eefd79d886b5792725ef0020dcfdcd/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/prettier@2.5.1/node_modules/prettier/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/prettier@2.5.1/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules:/home/workspace/react-hooks/sessions/node_modules:/home/workspace/react-hooks/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/eslint-plugin-prettier@3.3.1_74eefd79d886b5792725ef0020dcfdcd/node_modules/eslint-plugin-prettier/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/eslint-plugin-prettier@3.3.1_74eefd79d886b5792725ef0020dcfdcd/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../prettier@2.5.1/node_modules/prettier/bin-prettier.js" "$@"
else
  exec node  "$basedir/../../../../../prettier@2.5.1/node_modules/prettier/bin-prettier.js" "$@"
fi
